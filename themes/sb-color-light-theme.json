{
  "name": "sb-color[light]",
  "type": "light",
  "tokenColors": [
    {
      "scope": [
        // [comment]
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation",
        "keyword.other.documentation",
        // [phpdoc]
        "keyword.other.phpdoc",
        // [jsdoc]
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag",
      ],
      "settings": {
        "foreground": "#6d696488"
      }
    },
    {
      "scope": [
        // [variable]
        "variable",
        "constant",
        "support.variable",
        "meta.function-call.arguments",
        // [object prop]
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.object-literal.key",
        // [arrow func]
        "storage.type.function.arrow",
        // [punctuation]
        "punctuation",
        "keyword.operator",
        "meta.brace",
        // [css property]
        "support.type.property-name",
        // [tag attribute]
        "entity.other.attribute-name",
        "constant.numeric",
        // [class]
        "support.class",
        "meta.class",
        "support.other",
        // [css class]
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id",
        // [macros]
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#6d6964"
      }
    },
    {
      "scope": [
        // [jsx]
        "support.class.component",
        // [function]
        "entity.name.function",
        "support.function",
        "meta.function",
        "meta.function-call",
        // [markdown]
        "markup.quote",
        "markup.underline.link",
      ],
      "settings": {
        "foreground": "#2ebdb1",
      }
    },
    {
      "scope": [
        // [language keyword]
        "keyword",
        "storage",
        "storage.type.class",
        "storage.type.function",
        "variable.language",
        "constant.language",
        "punctuation.definition.template-expression",
        "punctuation.definition.variable",
        "keyword.operator.sizeof",
        "storage.type.annotation",
        "support.type.python",
        "storage.type.js",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "storage.type.trait",
        "storage.type.interface",
        // [css pseudo-*]
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class",
        // [lang std]
        "support.constant.std",
        // [markdown]
        "markup.inline",
        // [tag]
        "entity.name.tag",
        // [css property keyvalue]
        "support.constant.property-value",
        "support.constant.media",
      ],
      "settings": {
        "foreground": "#e6ac00",
      }
    },
    {
      "scope": [
        // [type]
        "keyword.other.type",
        "meta.other.type",
        "meta.type",
        "support.type",
        "storage.type",
        // [css pseudo]
        "entity.other.attribute-name.pseudo-element",
        "entity.other.attribute-name.pseudo-class"
      ],
      "settings": {
        "foreground": "#ff99be",
      }
    },
    {
      "scope": [
        // [string]
        "string",
        // [css hex color]
        "constant.other.color.rgb-value.hex.css"
      ],
      "settings": {
        "foreground": "#abc500",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        // [markdown italic]
        "markup.italic",
        // [comment]
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation",
        "keyword.other.phpdoc",
        // [jsdoc]
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag",
        // [object prop]
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.object-literal.key",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        // [markdown bold]
        "markup.bold",
        "markup.heading",
        // [doc]
        "keyword.other.phpdoc",
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag",
        "keyword.other.documentation",
        // [special character]
        "constant.character"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        // [markdown underline]
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        // [markdown link]
        "markup.underline.link"
      ],
      "settings": {
        "fontStyle": "italic underline"
      }
    }
  ],
