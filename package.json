{
    "name": "semantic-colors",
    "version": "1.0.0",
    "publisher": "sbovyrin",
    "displayName": "Semantic Colors",
    "description": "The color theme is inspired by nature and brings semantic meaning to syntax highlighting",
    "icon": "assets/icon.png",
    "galleryBanner": {
        "color": "#F5F7F8",
        "theme": "light"
    },
    "keywords": [
        "theme",
        "light",
        "dark",
        "syntax",
        "color"
    ],
    "license": "MIT",
    "engines": {
        "vscode": "^1.13.1"
    },
    "bugs": {
        "url": "https://github.com/sbovyrin/semantic-colors-theme/issues",
        "email": "sbovyrin@gmail.com"
    },
    "homepage": "https://github.com/sbovyrin/semantic-colors-theme/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/sbovyrin/semantic-colors-theme.git"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
              "label": "Semantic Colors [light]",
              "uiTheme": "vs",
              "path": "./themes/semantic-colors-light-theme.json"
            },
            {
              "label": "Semantic Colors [dark]",
              "uiTheme": "vs-dark",
              "path": "./themes/semantic-colors-dark-theme.json"
            }
        ]
    },
    "scripts": {
        "deploy": "vsce publish"
    },
    "devDependencies": {
        "vsce": "^1.43.0"
    }
}
